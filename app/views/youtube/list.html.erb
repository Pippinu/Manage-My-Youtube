<% item= JSON.parse(@mineresponse).fetch("items")[0] %>

<% snippet = item.fetch("snippet") %>
<% statistics = item.fetch("statistics")%>

<div class="spacer py-5"></div>
<div class="container pt-1 d-flex flex-row justify-content-center gap-3 px-5">
    <div>
        <img src="<%=snippet.fetch("thumbnails").fetch("high").fetch("url")%>" alt="avatar"
                    class="rounded-circle border" style="width: 150px;">
    </div>
    <div class="ytpicture d-flex flex-row px-5 gap-5">
        <div  class="d-flex flex-column justify-content-center">
            <h2 class="text-decoration-underline pb-3">Informazioni di <%= snippet.fetch("title")%></h2>
            <p><strong>Iscritti:</strong>  <%= statistics.fetch("subscriberCount")%></p>
            <p><strong>Numero di video:</strong>  <%= statistics.fetch("videoCount")%></p>
            <p><strong>Visualizzazioni del canale:</strong>  <%= statistics.fetch("viewCount")%></p>
        </div>
        
    </div>
</div>

<div class="container d-flex flex-row gap-3  pb-5 px-5">
    <div class="d-flex flex-column justify-content-center">
        <div class="py-4">
            <h3>Descrizione:</h3>
        </div>
        <div>
            <p><%= snippet.fetch("description")%></p>
        </div>
    </div>
</div>
